{
  "name": "story-atlas",
  "version": "0.1.0",
  "description": "A Library for interacting with Content Management Systems",
  "main": "src/index.js",
  "scripts": {
    "build": "webpack --config webpack.config.js",
    "prepublishOnly": "npm run build",
    "watch": "webpack --watch",
    "webpack": "webpack",
    "start": "webpack --watch & webpack-dev-server --open-page 'webpack-dev-server/dist'",
    "test": "mocha --compilers js:babel-core/register --require babel-polyfill --require babel-core/register --require ./test/config.js --recursive",
    "test:watch": "npm test -- --watch",
    "document": "documentation build src/** -f html -o docs --shallow"
  },
  "author": "Matthew Butler",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chai": "^4.2.0",
    "chai-url": "^1.0.4",
    "chai-json": "^1.0.0",
    "documentation": "^8.1.2",
    "mocha": "^5.2.0",
    "webpack": "^4.28.4",
    "webpack-cli": "^3.2.1",
    "webpack-dev-server": "^3.7.2"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ]
  },
  "dependencies": {
    "node-fetch": "^2.6.7"
  }
}
